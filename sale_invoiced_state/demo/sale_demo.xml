<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="sale_order_1" model="sale.order">
        <field name="partner_id" ref="base.res_partner_3" />
        <field name="partner_invoice_id" ref="base.res_partner_3" />
        <field name="user_id" ref="base.user_admin" />
        <field name="pricelist_id" ref="product.list0" />
        <field name="team_id" ref="sales_team.team_sales_department" />
        <field name="date_order" eval="DateTime.now() - relativedelta(months=2)" />
    </record>

    <record id="sale_order_line_1" model="sale.order.line">
        <field name="order_id" ref="sale_order_1" />
        <field name="name">Acoustic Bloc Screens</field>
        <field name="product_id" ref="product.product_product_22" />
        <field name="product_uom_qty">3</field>
        <field name="product_uom" ref="uom.product_uom_unit" />
        <field name="price_unit">2950.00</field>
    </record>

    <record id="sale_order_line_2" model="sale.order.line">
        <field name="order_id" ref="sale_order_1" />
        <field name="name">Raspberry Pi 4</field>
        <field name="product_id" ref="product.product_delivery_01" />
        <field name="product_uom_qty">5</field>
        <field name="product_uom" ref="uom.product_uom_unit" />
        <field name="price_unit">75.00</field>
    </record>
</odoo>
