<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

        <!-- Sequences for sale.order.generator -->
        <record id="seq_generator_sequence" model="ir.sequence">
            <field name="name">Generator Sequence</field>
            <field name="code">sale.order.generator</field>
            <field name="prefix">GEN-</field>
            <field name="padding" eval="5"></field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for sale.order in template mode -->
        <record id="seq_sale_order_tmpl_sequence" model="ir.sequence">
            <field name="name">Sale Order Template Sequence</field>
            <field name="code">sale.order.tmpl</field>
            <field name="prefix">TMPL-</field>
            <field name="padding" eval="5"></field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="dummy_so_generator_partner" model="res.partner">
            <field name="name">(technical) Generic customer for Sale Order Template</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

</odoo>
