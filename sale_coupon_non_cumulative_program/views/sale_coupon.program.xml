<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sale_coupon_program_view_promo_program_form" model="ir.ui.view">
        <field name="model">sale.coupon.program</field>
        <field
            name="inherit_id"
            ref="sale_coupon.sale_coupon_program_view_promo_program_form"
        />
        <field name="arch" type="xml">
            <field name="rule_date_to" position="after">
                <field name="is_cumulative" />
            </field>
        </field>
    </record>
    <record id="sale_coupon_program_view_tree" model="ir.ui.view">
        <field name="model">sale.coupon.program</field>
        <field name="inherit_id" ref="sale_coupon.sale_coupon_program_view_tree" />
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="is_cumulative" />
            </field>
        </field>
    </record>
</odoo>
