<odoo>

    <record model="ir.ui.view" id="product_category_form_view">
        <field name="name">product.category.form (in sale_product_country_filter)</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <group name="blacklist" string="Blacklisted countries">
                    <field
                        name="blacklisted_countries_ids"
                        nolabel="1"
                        />
                </group>
            </xpath>
        </field>
    </record>

